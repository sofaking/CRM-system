<div>Home</div>
<div>
  <% if session[:user_id] %>
    <%= "User is #{User.find(session[:user_id]).email}" %>
  <% else %>
    <%= "No user to-dos" %>
  <% end %>
  </div>
<div>
  <%= collection_check_boxes 'check', :todo_ids, Todo.all, :id, :title do |b| %>
    <%= b.check_box %><%= b.label id: b.value %><br/>
  <% end %>
</div>