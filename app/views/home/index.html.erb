<div>Home</div>
<div>
  <% if session[:user_id] %>
    <%= "User is #{User.find(session[:user_id]).email}" %>
  <% else %>
    <%= "No user to-dos" %>
  <% end %>
</div>

<h1>To-do lists</h1>

<div class="todo_lists">
  <%= render @todo_lists %>
</div>

<%= link_to "add todo list", todo_lists_path({ todo_list: { name: "New todo list" }, "commit"=>"Create Todo List"}), method: :create %>