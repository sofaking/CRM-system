<div class='all_todo_lists paper'>
  <div class='path'>
    <%= link_to 'Projects', current_user.account %> >
    <%= link_to @project.name, account_project_path(@project.account, @project) %> >
    <%= link_to 'To do lists', '' %>
  </div>
  <h1>Listing todo_lists</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @todo_lists.each do |todo_list| %>
        <tr>
          <td><%= todo_list.name %></td>
          <td><%= todo_list.description %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>