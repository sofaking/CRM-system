<div class='paper'>
  <h1>Listing todo_lists</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @todo_lists.each do |todo_list| %>
        <tr>
          <td><%= todo_list.name %></td>
          <td><%= todo_list.description %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>